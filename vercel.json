{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "node vercel-fix-deps.js && npm install --legacy-peer-deps --force",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "env": {
    "NEXT_PUBLIC_APP_URL": {
      "type": "string",
      "value": "https://$VERCEL_URL"
    }
  },
  "ignoreCommand": "echo $VERCEL_GIT_COMMIT_REF",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
} 